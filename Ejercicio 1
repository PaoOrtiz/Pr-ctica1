public class reino {
         private String reino;
         private String nombreEspecie;
         private String nombreComún;
         
public Reinos (String reino, String nombreEspecie, String nombreComún) {
         this.reino = reino;
         this.nombreEspecie = nombreEspecie;
         this.nombreComún = nombreComún;
         
}   

public String  getnombreEspecie() {
        return nombreEspecie;
}

public String getnombreComún (){
        return nombreComún;
}

public String mostrarDatos (){
return "Nombre del Reino: "+ reino+"\nombreEspecie: "+reino"\nnombreComún: " +nombreComún;
}
